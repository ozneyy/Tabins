{
  "manifest_version": 3,
  "name": "Tabin's",
  "version": "1.1",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "https://*.supabase.co/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "tabins@nzoy.script",
      "strict_min_version": "109.0",
      "data_collection_permissions": {
        "required": [
          "none"
        ]
      }
    }
  }
}